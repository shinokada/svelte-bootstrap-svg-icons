{
  "name": "svelte-bootstrap-svg-icons",
  "version": "1.0.3",
  "description": "Svelte Bootstrap SVG Icons for Svelte 5",
  "license": "MIT",
  "homepage": "https://svelte-bootstrap-svg-icons.codewithshin.com/",
  "author": {
    "name": "Shinichi Okada",
    "email": "connect@codewithshin.com",
    "url": "https://codewithshin.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinokada/svelte-bootstrap-svg-icons"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/shinokada/svelte-bootstrap-svg-icons/issues",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "gen:exports": "svelte-lib-helpers exports",
    "copy:packagejson": "svelte-lib-helpers package",
    "gen:docs5FromType": "svelte-lib-helpers docs5FromType",
    "lib-helpers": "npm run format && npm run gen:docs5FromType && npm run package && npm run gen:exports && npm run copy:packagejson"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@playwright/test": "^1.48.2",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.7.3",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "4.0.0",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "2.46.0",
    "globals": "^15.11.0",
    "highlight.js": "^11.10.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "publint": "^0.2.12",
    "runatics": "^0.1.3",
    "runes-meta-tags": "^0.4.0",
    "runes-webkit": "^0.9.36",
    "svelte": "5.1.6",
    "svelte-5-ui-lib": "^0.9.24",
    "svelte-check": "^4.0.5",
    "svelte-lib-helpers": "^0.4.16",
    "svelte-preprocess": "^6.0.3",
    "svelte-rune-highlight": "^0.5.12",
    "svelte-shields": "^0.5.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.14",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "keywords": [
    "bootstrap icons",
    "svelte",
    "runes",
    "sveltekit",
    "svg icons",
    "icons"
  ],
  "contributors": [
    {
      "name": "twbs/icons",
      "url": "https://github.com/twbs/icons"
    }
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  }
}
